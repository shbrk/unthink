CREATE DATABASE IF NOT EXISTS `<%-dbname%>` DEFAULT CHARACTER SET utf8 COLLATE utf8_general_ci;
USE `<%-dbname%>`;

-- --------------------------------------------------------

<%_ for(let node of mems) {_%>
--
-- 表的结构 `<%-node.name%>`
--

DROP TABLE IF EXISTS `<%-node.name%>`;
CREATE TABLE IF NOT EXISTS `<%-node.name%>` (
  <%_ for(let [idx,item] of node.mems.entries()) {_%>
  <%_ if(idx == node.mems.length - 1 && node.index == "") {_%>
  `<%-item.name%>` <%-item.type%><%-item.comment%>
  <%_} else {_%>
  `<%-item.name%>` <%-item.type%><%-item.comment%>,
  <%_}%><%_}%>  <%-node.index%>  
) ENGINE=InnoDB DEFAULT CHARSET=utf8<%-node.comment%>;

<%_}%>