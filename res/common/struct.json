{
    "DBObject": { // 所有入库对象应该继承此类 
        "uuid":"string"
    },
    "Item": { // 玩家物品
        "cid": "int",
        "count": "int",
        "sale": "float", //折扣
        "num":"int64",
        "price":"double",
        "prob":"object"
    },
    "UserInfo": {
        "<extends>": "DBObject",// 继承了DBObject 将会生成UserInfo表
        "name": "string", // 玩家姓名
        "level": "int",
        "bag": "array<Item>",
        "usingItem": "Item",
        "custom":"object",
        "info": { // 自定义格式
            "<type>": "string",
            "<dbType>": "text",
            "<default>": "abc"
        }
    }
}